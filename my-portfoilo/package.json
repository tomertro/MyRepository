  "jest-junit": {
    "outputDirectory": "<rootDir>/tests_out/",
    "outputName": "test_results.xml",
    "classNameTemplate": "AppsSuite.{classname}",
    "titleTemplate": "AppsSuite.{classname}.{title}",
    "suiteNameTemplate": "AppsSuite.{title}",
    "suiteName": "AppsSuite"
  }
