{
  "name": "apps-suite",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve --configuration=mock",
    "build": "ng lint && ng build --prod --stats-json ",
    "build-offline": "ng build --deleteOutputPath=true --configuration=offline",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "stats": "webpack-bundle-analyzer dist/stats.json",
    "build-sourcemap": "ng build --prod --sourcemaps --output-hashing none",
    "stats-sourcemap": "source-map-explorer dist/main.bundle.js",
    "test-debug": "ng test --browsers Chrome_with_debugging",
    "doc": "./node_modules/.bin/compodoc -p src/tsconfig.doc.json --includes additional-documentation -a additional-documentation/images"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.800.3",
    "@angular/animations": "^8.0.2",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "8.0.2",
    "@angular/compiler": "8.0.2",
    "@angular/core": "8.0.2",
    "@angular/forms": "8.0.2",
    "@angular/http": "7.2.15",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "8.0.2",
    "@angular/platform-browser-dynamic": "8.0.2",
    "@angular/platform-server": "8.0.2",
    "@angular/router": "8.0.2",
    "@babel/polyfill": "^7.4.4",
    "@ngrx/effects": "^8.0.1",
    "@ngrx/store": "^8.0.1",
    "@ngrx/store-devtools": "^8.0.1",
    "@ngx-translate/core": "11.0.1",
    "@types/loglevel": "^1.5.3",
    "angular-tslint-rules": "^1.1.0",
    "angular2-hotkeys": "2.1.4",
    "bi-app-sass": "^1.1.0",
    "bootstrap": "^4.2.1",
    "core-js": "^3.1.4",
    "date-fns": "^1.29.0",
    "deep-diff": "^1.0.2",
    "enhanced-resolve": "^4.0.0-beta.4",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "highcharts": "^7.1.2",
    "jquery": "^3.2.1",
    "lodash": "^4.17.11",
    "loglevel": "^1.6.3",
    "moment": "^2.20.1",
    "ng-mocks": "^8.0.0",
    "ngx-clipboard": "^12.1.2",
    "ngx-cookie-service": "^2.2.0",
